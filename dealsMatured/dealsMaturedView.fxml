<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane minHeight="550.0" minWidth="500.0" style="-fx-background-color: #030307;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dealsMatured.dealsMaturedViewController">
   <children>
      <Label layoutX="115.0" layoutY="23.0" prefHeight="36.0" prefWidth="231.0" text="     Deals Matured" textFill="SEAGREEN">
         <font>
            <Font name="Cambria Bold" size="29.0" />
         </font>
      </Label>
      <Label layoutX="29.0" layoutY="91.0" prefHeight="26.0" prefWidth="150.0" text="Buyer ID/Contact" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboBuyerID" layoutX="29.0" layoutY="131.0" onAction="#docomboBuyer" prefWidth="150.0" style="-fx-border-color: #2e8b57; -fx-border-width: 3;" />
      <Label layoutX="300.0" layoutY="91.0" prefHeight="26.0" prefWidth="150.0" text="Seller ID/Mobile" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboSellerID" layoutX="300.0" layoutY="131.0" onAction="#docomboSeller" prefWidth="150.0" style="-fx-border-color: #2e8b57; -fx-border-width: 3;" />
      <Label layoutX="29.0" layoutY="180.0" text="Property ID" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="300.0" layoutY="180.0" text="Total Amount" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtTotalAmount" layoutX="300.0" layoutY="221.0" style="-fx-border-color: #2e8b57; -fx-border-width: 3;" />
      <Label layoutX="29.0" layoutY="265.0" text="Advance Payment" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtAdvanceAmount" layoutX="29.0" layoutY="310.0" style="-fx-border-color: #2e8b57; -fx-border-width: 3;" />
      <Label layoutX="299.0" layoutY="265.0" text="Balence Amount" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="txtBalAmount" layoutX="301.0" layoutY="310.0" onKeyPressed="#doBalence" style="-fx-border-color: #2e8b57; -fx-border-width: 3;" />
      <Label layoutX="72.0" layoutY="361.0" prefHeight="26.0" prefWidth="359.0" text="Date Of Registration / Ownership Transfer" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="18.0" />
         </font>
      </Label>
      <DatePicker fx:id="dateReg" layoutX="132.0" layoutY="402.0" style="-fx-border-color: #2e8b57; -fx-border-width: 3;" />
      <Button layoutX="173.0" layoutY="468.0" mnemonicParsing="false" onAction="#doSave" prefHeight="38.0" prefWidth="114.0" style="-fx-background-color: #2e8b57;" text="Save" textFill="WHITE">
         <font>
            <Font name="Cambria Bold" size="20.0" />
         </font>
      </Button>
      <ComboBox fx:id="comboPropertyID" layoutX="29.0" layoutY="221.0" onAction="#docomboProperty" prefWidth="150.0" style="-fx-border-color: #2e8b57; -fx-border-width: 3;" />
   </children>
</AnchorPane>
